        "asdf" : {
          "description" : "asdf",
          "type" : "integer",
          "minimum" : 0,
          "maximum" : 255
        }
      }
    },
    "PLACEHOLDER_2" : {
      "type" : "object",
      "description" : "Structure for PLACEHOLDER_ packet, as single raw hex string.",
