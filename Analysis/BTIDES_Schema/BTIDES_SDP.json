{
  "$id" : "https://darkmentor.com/BTIDES_Schema/BTIDES_SDP.json",
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "description" : "BTIDES SDP packet information data types",
  "type" : "object",
  "properties" : {
    "SDPArray" : {
      "type" : "array",
      "items" : {
        "anyOf" : [ {
          "$ref" : "#/definitions/PLACEHOLDER_"
